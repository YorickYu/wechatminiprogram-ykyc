.backgroundcolor {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
  z-index: 0;
}

.container {
  position: relative;
  display: flex;
  flex-direction: column;
  z-index: 5;
}

.top-container {
  border-bottom: 5rpx solid #f8f8f8;
}


.item {
  background-color: white;
  border-bottom: .5rpx solid #f8f8f8;
  padding: 0 30rpx;

  display: flex;
  justify-content: space-between;
}

.item .left-item {
  color: #7d7d7d;
  font-size: 34rpx;
  line-height: 100rpx;
}
.item .right-item {
  color: #111111;
  font-size: 30rpx;
  max-width: 70%;
  line-height: 60rpx;
  margin: 20rpx 0;
  /* white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; */
}
