
#myVideo {
  width: 100%;
  height: 500rpx;
  margin-top: 100rpx;
}

.video-container {
  position: fixed;
  width: 100%;
  background-color: #111111;
}

.count-contaienr {
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  margin-left: -215rpx;
  width: 450rpx;
  height: 110rpx;
  border-radius: 55rpx;
  color: white;
  font-size: 32rpx;

  display: flex;
  justify-content: center;
  align-items: center;
}

.count-contaienr view {
  text-align: center;
  padding: 5rpx 40rpx;
}

.active-color{
    background-color: #dc228a;
}

.unactive-color{
  background-color: #979797;
}


.controls {
  position: relative;
  top: 20%;
  left: 80%;
  height: 50px;
  width: 100rpx;
}

.time {
  text-align: center;
  background-color: rgba(0, 0, 0, 0.5);
  color: white;
  line-height: 50px;
  height: 50px;
  width: 100rpx;
}