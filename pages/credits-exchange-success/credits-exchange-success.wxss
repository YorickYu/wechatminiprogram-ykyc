/* pages/credits-exchange-success/credits-exchange-success.wxss */
.ces_top_top {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 380rpx;
}
.ces_top_top .success_img {
  margin-top: 100rpx;
  width: 200rpx;
  height: 260rpx;
}
.ces_top_mid {
  display: flex;
  justify-content: center;
  text-align: center;
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 56rpx;
  font-weight: bold;
}
.ces_middle {
  margin-top: 100rpx;
}
.ces_middle .one {
  flex-direction: row;
}
.ces_middle .one .good_name_ {
  margin-left: 64rpx;
  font-size: 30rpx;
  color: #7D7D7D;
}
.ces_middle .one .good_name {
  margin-left: 24rpx;
  font-size: 30rpx;
  color: #7D7D7D;
}
.ces_middle .two {
  flex-direction: row;
}
.ces_middle .two .exchange_time_ {
  margin-left: 64rpx;
  font-size: 30rpx;
  color: #7D7D7D;
}
.ces_middle .two .exchange_time {
  margin-left: 24rpx;
  font-size: 30rpx;
  color: #7D7D7D;
}
.ces_middle .three {
  flex-direction: row;
}
.ces_middle .three .exchange_score_ {
  margin-left: 64rpx;
  font-size: 30rpx;
  color: #7D7D7D;
}
.ces_middle .three .exchange_score {
  margin-left: 24rpx;
  font-size: 30rpx;
  color: #2B2B2B;
  font-weight: 600;
}
.ces-finish {
  margin: 20rpx;
  background-color: rgb(211, 0, 120);
  width: 90%;
  height: 100rpx;
  border-radius: 100rpx;
  color: white;
  text-align: center;
  line-height: 100rpx;
}
.ces-bottom {
  margin-top: 130rpx;
  display: flex;
  justify-content: center;
}
.ces-bottom .finish {
  margin: 20rpx;
  background-color: rgb(211, 0, 120);
  width: 480rpx;
  height: 100rpx;
  border-radius: 100rpx;
  color: white;
  text-align: center;
  line-height: 100rpx;
}